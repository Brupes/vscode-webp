{
  "name": "webp-converter",
  "displayName": "WebP Converter",
  "description": "Convert images into the WebP format.",
  "version": "0.1.0",
  "publisher": "axelrindle",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "webp",
    "convert",
    "image",
    "converter",
    "png",
    "jpg",
    "jpeg",
    "tiff",
    "webp-convert",
    "webp-converter",
    "cwebp"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/axelrindle/vscode-webp.git"
  },
  "homepage": "https://github.com/axelrindle/vscode-webp#readme",
  "icon": "logo.png",
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "webp-converter.execute",
        "title": "Convert to WebP",
        "enablement": "resourceScheme == file && resourceExtname =~ /.(png|jpg|jpeg|tiff)/i"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "webp-converter.execute",
          "group": "7_modification@99"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.76.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.2.3"
  }
}
